		<Description>A project that diagnostics about the solution codes.</Description>
		<DocumentationFile>..\docxml\Sudoku.Diagnostics.CodeAnalysis.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<AdditionalFiles Include="DiagnosticResults.csv" />
	</ItemGroup>

	<ItemGroup>
