<Window
    x:Class="SudokuStudio.Views.Windows.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
	Activated="Window_Activated" Closed="Window_Closed">

	<NavigationView
		PaneTitle="Main Menu" IsBackButtonVisible="Collapsed" IsSettingsVisible="False" IsTabStop="False"
		IsPaneOpen="True" IsPaneToggleButtonVisible="False">
		<NavigationView.MenuItems>
			<NavigationViewItem Content="Analyze" Icon="ShowResults" />
		</NavigationView.MenuItems>
	</NavigationView>
</Window>
