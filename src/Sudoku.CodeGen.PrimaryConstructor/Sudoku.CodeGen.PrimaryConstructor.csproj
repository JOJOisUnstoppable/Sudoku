<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>9.0</LangVersion>
		<Nullable>enable</Nullable>
		<Version>0.1</Version>
		<DocumentationFile>..\docxml\Sudoku.CodeGen.PrimaryConstructor.xml</DocumentationFile>
		<Authors>Sunnie</Authors>
		<Description>A source generator project that generates the code about the primary constructors of a class.</Description>
		<Copyright>Copyright (c) Sunnie 2021</Copyright>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="IsExternalInit" Version="1.0.0">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.CodeAnalysis.Common" Version="3.10.0-1.final" />
		<PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.10.0-1.final" />
	</ItemGroup>

</Project>
