<Application
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="Sudoku.Maui.App">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Resources/Styles/Colors.xaml" />
				<ResourceDictionary Source="Resources/Styles/Styles.xaml" />
				<ResourceDictionary Source="Resources/Text/Resources.xaml" />
				<ResourceDictionary Source="Resources/Text/Resources-zh-CN.xaml" />
				<ResourceDictionary Source="Resources/Text/Resources-ja-JP.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
