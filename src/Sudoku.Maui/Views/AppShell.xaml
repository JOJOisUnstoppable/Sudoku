<Shell
	x:Class="Sudoku.Maui.Views.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:v="clr-namespace:Sudoku.Maui.Views"
	xmlns:vm="clr-namespace:Sudoku.Maui.ViewModels"
	Shell.FlyoutBehavior="Disabled">
	<Shell.BindingContext>
		<vm:AppShellViewModel />
	</Shell.BindingContext>

	<ShellContent Title="Home" ContentTemplate="{DataTemplate v:MainPage}" Route="MainPage" />
</Shell>
