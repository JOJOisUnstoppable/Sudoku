<Page
	x:Class="Sudoku.UI.Pages.MainWindow.SudokuPanelPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:interactions="using:Sudoku.UI.Interactions"
	xmlns:c="using:Sudoku.UI.CustomControls"
	mc:Ignorable="d"
	Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
	<Page.DataContext>
		<interactions:SudokuPanelPageModel x:Name="ViewModel" />
	</Page.DataContext>

	<c:SudokuPanel x:Name="SudokuPanel_Main" />
</Page>
