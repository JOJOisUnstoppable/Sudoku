<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Description>The project that provides human-analysis or brute forces for sudoku puzzles.</Description>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<DocumentationFile>..\docxml\Sudoku.Solving.xml</DocumentationFile>
		<RootNamespace>Sudoku</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Sudoku.Core\Sudoku.Core.csproj" />
		<ProjectReference Include="..\Sudoku.CodeGenerating\Sudoku.CodeGenerating.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="true" />
	</ItemGroup>

	<ItemGroup>
		<EditorConfigFiles Remove="..\Sudoku.Solving\.editorconfig" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\Sudoku.Solving\.editorconfig" />
	</ItemGroup>

</Project>
