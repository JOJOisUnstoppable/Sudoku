<UserControl x:Class="Sudoku.Windows.CustomControls.SudokuGridPanel"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:sys="clr-namespace:System;assembly=System.Runtime"
			 mc:Ignorable="d" d:DesignHeight="600" d:DesignWidth="600">
	<UserControl.Resources>
		<sys:Double x:Key="DefaultWidth">600</sys:Double>
		<sys:Double x:Key="DefaultHeight">600</sys:Double>
	</UserControl.Resources>
	<Grid>
		<Image x:Name="_mainGrid" Width="{StaticResource DefaultWidth}" Height="{StaticResource DefaultHeight}" />
	</Grid>
</UserControl>
